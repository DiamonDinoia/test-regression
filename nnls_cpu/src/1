#include "io/interface/ecal_data_io.h"

int main() {
    if (argc>=2)
        std::string file_name {args[1]};

    ecal_data_io io;
    auto entries = io.get_n_entries(100);
    for (auto const& [inputs, pulse_matrix] : entries) {
        std::cout << inputs;
        std::cout << pulse_matrix;
    }

    return 0;
}
